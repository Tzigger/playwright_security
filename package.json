{
  "name": "@tzigger/kinetic",
  "version": "0.2.0",
  "description": "Kinetic - High-performance Dynamic Application Security Testing (DAST) engine built on Playwright",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "kinetic": "dist/cli/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:juice-sqli": "npx playwright test tests/juice-shop-sqli.spec.ts --project=chromium --reporter=line",
    "test:juice-owasp": "npx playwright test tests/owasp-top10-juice-shop.spec.ts --project=chromium --reporter=line",
    "test:juice-sqli:watch": "chokidar 'src/**/*.ts' 'tests/**/*.ts' -c 'npm run build && npm run test:juice-sqli'",
    "test:juice-owasp:watch": "chokidar 'src/**/*.ts' 'tests/**/*.ts' -c 'npm run build && npm run test:juice-owasp'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test:phase2": "npx playwright test tests/integration/phase2-comprehensive.test.ts --reporter=list --project=chromium"
  },
  "keywords": [
    "kinetic",
    "dast",
    "security",
    "testing",
    "playwright",
    "vulnerability",
    "scanner",
    "penetration-testing",
    "xss",
    "sql-injection",
    "security-audit",
    "security-testing",
    "web-security",
    "appsec",
    "devsecops",
    "owasp",
    "sarif"
  ],
  "author": "Tzigger",
  "license": "MIT",
  "homepage": "https://github.com/Tzigger/playwright_security#readme",
  "bugs": {
    "url": "https://github.com/Tzigger/playwright_security/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tzigger/playwright_security.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "handlebars": "^4.7.8",
    "ora": "^5.4.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  }
}
